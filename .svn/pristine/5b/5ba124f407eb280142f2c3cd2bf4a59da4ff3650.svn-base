package com.fh.entity.system;

import java.io.Serializable;

/**
 * 按钮类
 * */
public class Button implements Serializable {

    private static final long serialVersionUID = 1L;
    private Long BUT_ID;
    private String BUT_NAME;
    private String BUT_CODE;
    private String BUT_VALUE;
    private String BUT_SORT;
    private boolean hasButton;

    public Long getBUT_ID() {
        return BUT_ID;
    }

    public void setBUT_ID(Long bUT_ID) {
        BUT_ID = bUT_ID;
    }

    public String getBUT_NAME() {
        return BUT_NAME;
    }

    public void setBUT_NAME(String bUT_NAME) {
        BUT_NAME = bUT_NAME;
    }

    public String getBUT_CODE() {
        return BUT_CODE;
    }

    public void setBUT_CODE(String bUT_CODE) {
        BUT_CODE = bUT_CODE;
    }

    public String getBUT_VALUE() {
        return BUT_VALUE;
    }

    public void setBUT_VALUE(String bUT_VALUE) {
        BUT_VALUE = bUT_VALUE;
    }

    public String getBUT_SORT() {
        return BUT_SORT;
    }

    public void setBUT_SORT(String bUT_SORT) {
        BUT_SORT = bUT_SORT;
    }

    public boolean isHasButton() {
        return hasButton;
    }

    public void setHasButton(boolean hasButton) {
        this.hasButton = hasButton;
    }

}
